{
  "name": "@storybook-astro/renderer",
  "version": "0.1.0-beta.10",
  "description": "Client-side renderer for Storybook Astro framework",
  "module": "./src/preset.ts",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemcd/storybook-astro.git",
    "directory": "packages/@storybook-astro/renderer"
  },
  "homepage": "https://storybook-astro.org",
  "bugs": {
    "url": "https://github.com/lukemcd/storybook-astro/issues"
  },
  "engines": {
    "node": ">=20.16.0 || >=22.19.0 || >=24.0.0"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": "./src/preset.ts",
    "./package.json": "./package.json",
    "./preset": "./src/preset.ts",
    "./types": "./src/types.ts",
    "./*": "./src/*"
  },
  "publishConfig": {
    "module": "./dist/preset.js",
    "exports": {
      ".": {
        "types": "./src/preset.ts",
        "import": "./dist/preset.js"
      },
      "./package.json": "./package.json",
      "./preset": {
        "types": "./src/preset.ts",
        "import": "./dist/preset.js"
      },
      "./types": {
        "types": "./src/types.ts",
        "import": "./dist/types.js"
      },
      "./*": "./src/*"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "tsup"
  },
  "devDependencies": {
    "astro": "^5.6.1 || ^6.0.0-beta.0"
  },
  "peerDependencies": {
    "@storybook/react": "^10.0.0",
    "astro": "^5.5.3 || ^6.0.0-beta.0",
    "storybook": "^10.0.0"
  },
  "dependencies": {
    "@types/node": "^20.19.0"
  },
  "peerDependenciesMeta": {
    "@storybook/react": {
      "optional": true
    }
  }
}
